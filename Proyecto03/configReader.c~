#include <stdio.h>
#include <stdlib.h>
#define MAX_LENGTH 100

int configReader(int carMean, int carCounter) {

    char line[MAX_LENGTH];
    FILE *fp = fopen("config.txt", "r");

    if (! fp) 
       return EXIT_FAILURE;

    int numread = 0;
    while (fgets(line, MAX_LENGTH, fp)) {
        numread += sscanf(line, "carMean = %d", carMean);
        numread += sscanf(line, "carCounter = %d", carCounter);
    }
    fclose(fp);

    return numread;
}
